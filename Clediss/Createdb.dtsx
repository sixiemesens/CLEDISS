T NULL,&#xA;  [soussociete_code] varchar(255) DEFAULT NULL,&#xA;  [type] varchar(255) NOT NULL,&#xA;  [max_bl] int ,&#xA;  [max_cmd] int ,&#xA;  [max_devis] int ,&#xA;  [max_recouvrement] int ,&#xA;  [max_client] int ,&#xA;  [dash_max_avoir] int NULL DEFAULT '0',&#xA;  [dash_max_bl] int DEFAULT NULL,&#xA;  [Paiement_Esp] varchar(255) DEFAULT NULL,&#xA;  [autorisation_client] varchar(1) DEFAULT '0',&#xA;  [autorisation_modification_client] varchar(1) ,&#xA;  [remise] float NOT NULL DEFAULT '0',&#xA;  [avoir] float NOT NULL DEFAULT '0',&#xA;  [consigne] int DEFAULT '0',&#xA;  [sync_clients] varchar(50),&#xA;  [plafond_credit] float NOT NULL DEFAULT '0',&#xA;  [vente_credit] varchar(1) DEFAULT '0',&#xA;  [obligation_achat_avoir] varchar(1) NOT NULL DEFAULT '0',&#xA;  [colisage] int DEFAULT '0',&#xA;  [deleted_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [created_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [updated_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [update_remise] int DEFAULT '0',&#xA;  [bloquage_commande] varchar(1) NOT NULL DEFAULT '0',&#xA;  [version] varchar(10) DEFAULT NULL,&#xA;  [livraison] varchar(1) NOT NULL DEFAULT '0',&#xA;  [district] varchar(255) NOT NULL&#xA;)  ;&#xA;&#xA;&#xA;-- Structure de la table `entetecommercials`&#xA;if not exists&#xA;(select * from sysobjects where name='entetecommercials' and xtype='U')&#xA;CREATE TABLE entetecommercials (&#xA;  [id] int ,&#xA;  [code] varchar(255),&#xA;  [code_commande] varchar(255) DEFAULT NULL,&#xA;  [code_a_barre] varchar(255) DEFAULT NULL,&#xA;  [facture_code] varchar(255) DEFAULT NULL,&#xA;  [visite_code] varchar(255) DEFAULT NULL,&#xA;  [type] varchar(50),&#xA;  [envoi_cmd] int NOT NULL DEFAULT '0',&#xA;  [societe_code] varchar(255) DEFAULT NULL,&#xA;  [soussociete_code] varchar(255) DEFAULT NULL,&#xA;  [user_code] varchar(255) DEFAULT NULL,&#xA;  [depot_code] varchar(255) DEFAULT NULL,&#xA;  [client_code] varchar(255) DEFAULT NULL,&#xA;  [caisse_code] varchar(191) DEFAULT NULL,&#xA;  [client_nom] varch