          SQLTask:SqlStatementSource="-- Création des tables de la Base de données STGCLEDISS&#xA;&#xA;USE [STGCLEDISS];&#xA;&#xA;-- Structure de la table `activites`&#xA;if not exists&#xA;(select * from sysobjects where name='activites' and xtype='U')&#xA;CREATE TABLE activites (&#xA;  [id] int ,&#xA;  [code] varchar(255) NOT NULL,&#xA;  [libelle] varchar(255) NOT NULL,&#xA;  [created_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [updated_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [district] varchar(255) NOT NULL&#xA;)     ;&#xA;&#xA;-- Structure de la table `affectation_clients`&#xA;if not exists&#xA;(select * from sysobjects where name='affectation_clients' and xtype='U')&#xA;CREATE TABLE affectation_clients (&#xA;  [id] int ,&#xA;  [promotion_code] varchar(255) DEFAULT NULL,&#xA;  [gratuite_code] varchar(255) DEFAULT NULL,&#xA;  [fidelite_code] varchar(255) DEFAULT NULL,&#xA;  [categorie_code] varchar(255) DEFAULT NULL,&#xA;  [region_code] varchar(255) DEFAULT NULL,&#xA;  [zone_code] varchar(255) DEFAULT NULL,&#xA;  [type] varchar(255) DEFAULT NULL,&#xA;  [deleted_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [created_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [updated_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [district] varchar(255) NOT NULL&#xA;)  ;&#xA;&#xA;-- Structure de la table `caisses`&#xA;if not exists&#xA;(select * from sysobjects where name='caisses' and xtype='U')&#xA;CREATE TABLE caisses (&#xA;  [id] int ,&#xA;  [code] varchar(100) NOT NULL,&#xA;  [user_code] varchar(100) DEFAULT NULL,&#xA;  [depot_code] varchar(100) DEFAULT NULL,&#xA;  [recette] float NOT NULL DEFAULT '0',&#xA;  [journee] float NOT NULL DEFAULT '0',&#xA;  [espece] float NOT NULL DEFAULT '0',&#xA;  [cheque] float NOT NULL DEFAULT '0',&#xA;  [credit] float NOT NULL DEFAULT '0',&#xA;  [depense] float NOT NULL DEFAULT '0',&#xA;  [gratuite] float NOT NULL DEFAULT '0',&#xA;  [recouvrement] float NOT NULL DEFAULT '0',&#xA;  [banque] float NOT NULL DEFAULT '0',&#xA;  [versement_banque] float NOT NULL DEFAULT '0',&#xA;  [remise] float NOT NULL DEFAULT '0',&#xA;  [ecart] float NOT NULL DEFAULT '0',&#xA;  [date] date DEFAULT NULL,&#xA;  [cloture] smallint NOT NULL DEFAULT '0',&#xA;  [note] varchar(50) DEFAULT NULL,&#xA;  [created_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [updated_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [district] varchar(255) NOT NULL&#xA;)    ;&#xA;&#xA;-- Structure de la table `carburants`&#xA;if not exists&#xA;(select * from sysobjects where name='carburants' and xtype='U')&#xA;CREATE TABLE carburants (&#xA;  [id] int ,&#xA;  [code] varchar(255),&#xA;  [user_code] varchar(255),&#xA;  [depot_code] varchar(255),&#xA;  [ticket_code] varchar(255) DEFAULT NULL,&#xA;  [montant] float NOT NULL DEFAULT '0',&#xA;  [date] date,&#xA;  [longitude] varchar(50),&#xA;  [latitude] varchar(50),&#xA;  [created_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [updated_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [district] varchar(255) NOT NULL&#xA;)   ;&#xA;&#xA;-- Structure de la table `chargement_type`&#xA;if not exists&#xA;(select * from sysobjects where name='chargement_type' and xtype='U')&#xA;CREATE TABLE chargement_type (&#xA;  [id] int,&#xA;  [tournee_code] varchar(255),&#xA;  [produit_code] varchar(255),&#xA;  [quantite] varchar(255),&#xA;  [date_debut] date,&#xA;  [date_fin] date,&#xA;  [created_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [updated_at] datetime2(0) NULL DEFAULT NULL,&#xA;  [district] varchar(255) NOT NULL&#xA;)     ;&#xA;&#xA;-- Structure de la table `clients`&#xA;if not exists&#xA;(select * from sysobjects where name='clients' and xtype='U')&#xA;CREATE TABLE clients (&#xA;  [id] int,&#xA;  [code] varchar(255),&#xA;  [code_a_barre] varchar(255),&#xA;  [activite_code] varchar(255) DEFAULT NULL,&#xA;  [user_code] varchar(255) DEFAULT NULL,&#xA;  [soussociete_code] varchar(150) DEFAULT NULL,&#xA;  [nom] varchar(255),&#xA;  [prenom] varchar(250) DEFAULT NULL,&#xA;  [statut] varchar(250) DEFAULT NULL,&#xA;  [passager] varchar(1) DEFAULT '0',&#xA;  [